# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=your_database_password
DATABASE_NAME=agentic_db
# Alternative: Full DATABASE_URL (if using connection string)
# DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=300

# Cache TTL Configuration (in seconds)
CACHE_TTL_USER=3600
CACHE_TTL_USER_EMAIL=3600
CACHE_TTL_CONVERSATION=600
CACHE_TTL_CONVERSATION_LIST=300
CACHE_TTL_MESSAGES=600
CACHE_TTL_FILES=3600
CACHE_TTL_EMBEDDING=86400
CACHE_TTL_SEARCH=3600
CACHE_TTL_TOOL=86400
CACHE_TTL_SESSION=86400
CACHE_TTL_ANALYTICS=2592000

# Rate Limiting Configuration
RATE_LIMIT_WINDOW=60
RATE_LIMIT_CHAT_MESSAGE=60
RATE_LIMIT_CHAT_MESSAGE_WINDOW=60
RATE_LIMIT_CHAT_CREATE=20
RATE_LIMIT_CHAT_CREATE_WINDOW=60
RATE_LIMIT_FILE_UPLOAD=10
RATE_LIMIT_FILE_UPLOAD_WINDOW=60
RATE_LIMIT_API_GENERAL=100
RATE_LIMIT_API_GENERAL_WINDOW=60
RATE_LIMIT_UNAUTH=50
RATE_LIMIT_UNAUTH_WINDOW=60

# Stream Configuration
STREAM_TIMEOUT=300

# Server Configuration
PORT=3000
NODE_ENV=development

# AI Engine URL
# Point to your AI agent service
AI_ENGINE_URL=http://localhost:8001

# CORS Configuration
# Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:5178,http://localhost:3001

# Vercel Blob Storage (for file uploads)
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Demo Mode Configuration
# UUID for demo user (if using demo mode)
DEMO_USER_ID=
